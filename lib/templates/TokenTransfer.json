{
  "type": "Transfer",
  "variables": {
    "amount": {
      "data_point": "data",
      "function": {
        "name": "formatNumber",
        "args": [
          {
            "name": "number",
            "path": "token_transfers[0].total.value"
          },
          {
            "name": "decimal",
            "path": "token_transfers[0].total.decimals"
          }
        ]
      }
    },
    "tokenAddress": {
      "data_point": "data",
      "path": "token_transfers[0].token.address"
    },
    "toAddress": {
      "data_point": "data",
      "path": "token_transfers[0].to.hash"
    }
  },
  "message": "Transfer {amount} {tokenAddress} To {toAddress}"
}
