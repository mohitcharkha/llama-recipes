{
  "type": "Approve",
  "triggers": [
    {
      "data_point": "events",
      "path": "items.length",
      "condition": "equals",
      "value": "1"
    },
    {
      "data_point": "events",
      "path": "items[0].temp_function_name",
      "condition": "presentIn",
      "value": ["Approval", "ApprovalForAll"]
    },
    {
      "data_point": "data",
      "path": "decoded_input",
      "condition": "notEquals",
      "value": null
    },
    {
      "data_point": "data",
      "path": "decoded_input.parameters[1].value",
      "condition": "notPresentIn",
      "value": ["0", "false"]
    }
  ],
  "variables": {
    "contractAddress": {
      "data_point": "data",
      "path": "to.hash"
    },
    "spendor": {
      "data_point": "data",
      "path": "decoded_input.parameters[0].value"
    }
  },
  "message": "Approved {contractAddress} For Trade On {spendor}"
}